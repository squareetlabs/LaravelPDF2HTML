{
  "name": "squareetlabs/laravel-pdf-to-html",
  "description": "Laravel package to convert PDF to HTML without external dependencies",
  "keywords": [
    "laravel",
    "pdf",
    "html",
    "converter",
    "pdftohtml",
    "poppler",
    "pdf2html",
    "export",
    "transform"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alberto Rial Barreiro",
      "email": "alberto@squareet.com",
      "homepage": "https://www.squareet.com",
      "role": "Developer"
    },
    {
      "name": "Jacobo Cantorna CigarrÃ¡n",
      "email": "jacobo@squareet.com",
      "homepage": "https://www.squareet.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Squareetlabs\\LaravelPdfToHtml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Squareetlabs\\LaravelPdfToHtml\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Squareetlabs\\LaravelPdfToHtml\\PdfToHtmlServiceProvider"
      ],
      "aliases": {
        "PdfToHtml": "Squareetlabs\\LaravelPdfToHtml\\Support\\Facades\\PdfToHtml"
      }
    }
  },
  "minimum-stability": "stable"
}
